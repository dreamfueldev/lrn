{
  "name": "@lrn/adapters",
  "version": "0.0.1",
  "description": "Adapters for parsing OpenAPI, TypeScript, and other formats",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./openapi": {
      "types": "./dist/openapi.d.ts",
      "import": "./dist/openapi.js"
    },
    "./typescript": {
      "types": "./dist/typescript.d.ts",
      "import": "./dist/typescript.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run"
  },
  "dependencies": {
    "@lrn/schema": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "vitest": "^2.0.0"
  }
}
